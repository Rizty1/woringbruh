  <div class="modal modal-success fade" id="stock_modal" tabindex="-1" role="dialog" aria-labelledby="modal_success"
    aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title h6" id="modal_title_6">Check our stock</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="py-3 text-center">
            <i class="far fa-users fa-4x"></i>
            <h5 class="heading h4 mt-4">Upgrade Stock</h5>
