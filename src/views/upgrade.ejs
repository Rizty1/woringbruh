            <hr class="divider divider-fade" />
            <a href="#" id="inviteurl" target="_blank" class="btn btn-animated btn-success btn-animated-x">
              <span class="btn-inner--visible">Upgrade</span>
              <span class="btn-inner--hidden">
                <i class="far fa-arrow-right"></i>
              </span>
