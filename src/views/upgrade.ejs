  <div class="main-content">
    <!-- Header (v5) -->
    <section class="slice slice-xl pb-250 bg-primary" data-offset-top="#header-main">
      <div class="bg-absolute-cover bg-size--contain d-flex align-items-center zindex-100">
        <figure class="w-100">
          <img alt="Image placeholder" src="/assets/img/svg/backgrounds/bg-1.svg" class="svg-inject">
        </figure>
      </div>
      <div class="spotlight-holder d-flex align-items-center">
        <div class="container position-relative zindex-100 py-7">
          <div class="row row-grid justify-content-around align-items-center">
            <div class="col-lg-6">
              <div class="pt-lg text-center">
                <h2 class="h1 text-white mb-3">Upgrade to <span class="text-info typed" id="type-example"
                    data-type-this="no ads., unlimited skips., offline streaming., Spotify Premium."></span></h2>
                <p class="lead text-white lh-180">Enter your <a href="/buy" class="text-dark">upgrade key</a> and
                  country to enjoy the benefits of Spotify Premium at no extra cost.</p>
                <div class="mt-5 px-4">
                  <form class="mt-5">
                    <div class="form-group mb-0">
                      <div class="input-group input-group-merge rounded-pill bg-white">
                        <select class="form-control form-control-flush" id="countryoptions"
                          style=" border-top-right-radius: 50rem;border-bottom-right-radius: 50rem;"></select>
                      </div>
                    </div>
                    <br>
                    <div class="form-group mb-0">
                      <div class="input-group input-group-merge rounded-pill bg-white">
                        <input type="password" class="form-control form-control-flush" name="upgradekey" id="upgradekey"
                          placeholder="Upgrade key">
                      </div>
                    </div>
                    <div class="mt-4">
                      <a href="/buy" class="text-dark">Buy a key</a>
                      <button style="margin-left: 10px;" type="button" id="submit"
                        class="btn btn-sm btn-info btn-icon rounded-pill">
                        <span class="btn-inner--text">Upgrade</span>
                        <span class="btn-inner--icon"><i class="far fa-long-arrow-alt-right"></i></span>
                      </button>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="shape-container" data-shape-position="bottom">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1600 220" preserveAspectRatio="none"
